{
    "name": "html-minifiers-benchmark",
    "version": "0.0.0",
    "description": "Benchmark of various HTML minifiers",
    "type": "module",
    "main": "index.js",
    "engines": {
        "node": ">=18.0.0"
    },
    "author": "Kirill Maltsev <maltsevkirill@gmail.com>",
    "license": "MIT",
    "scripts": {
        "lint": "eslint . && prettier --check .",
        "lint:fix": "eslint . --fix && prettier --write .",
        "prebenchmark": "rm -rf ./build/ && mkdir ./build/",
        "benchmark": "node index.js",
        "postbenchmark": "prettier --write README.md"
    },
    "dependencies": {
        "@minify-html/node": "latest",
        "@tdewolff/minify": "latest",
        "handlebars": "^4.7.7",
        "html-minifier-next": "latest",
        "html-minifier-terser": "latest",
        "htmlnano": "latest"
    },
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "eslint": "^9.0.0",
        "prettier": "^3.6.2"
    },
    "peerDependencies": {
        "cssnano": "^7.0.0",
        "postcss": "^8.3.11",
        "purgecss": "^7.0.2",
        "relateurl": "^0.2.7",
        "srcset": "5.0.1",
        "svgo": "^3.0.2",
        "terser": "^5.10.0",
        "uncss": "^0.17.3"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/maltsev/html-minifiers-benchmark.git"
    },
    "bugs": {
        "url": "https://github.com/maltsev/html-minifiers-benchmark/issues"
    },
    "homepage": "https://github.com/maltsev/html-minifiers-benchmark"
}
